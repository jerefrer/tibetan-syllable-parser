{
  "name": "tibetan-syllable-parser",
  "version": "1.0.1",
  "description": "A parser to deconstruct a single Unicode Tibetan syllable into its different parts.",
  "main": "dist/tibetan-syllable-parser.umd.js",
  "scripts": {
    "clean": "rm -rf ./dist && mkdir dist",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "build:esm": "cp src/tibetan-syllable-parser.js dist/tibetan-syllable-parser.esm.js",
    "build:umd": "rollup src/tibetan-syllable-parser.js --format umd --name TibetanSyllableParser --file dist/tibetan-syllable-parser.umd.js",
    "test": "jest --colors"
  },
  "keywords": [
    "tibetan",
    "syllables",
    "languages",
    "linguistics",
    "transliteration",
    "phonetics"
  ],
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "rollup": "^4.2.0"
  },
  "dependencies": {
    "underscore": "^1.13.6"
  }
}
